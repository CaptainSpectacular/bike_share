<%= form_for [:admin, @accessory] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :price%>
  <%= f.number_field :price %>

  <%= f.label :image %>
  <%= f.text_field :image %>

  <%= f.label :status %>
  <%= f.text_field :status %>

  <%= f.submit %>
<% end %>
